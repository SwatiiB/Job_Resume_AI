{
  "name": "resume-refresh-platform",
  "version": "1.0.0",
  "description": "AI-Powered Resume Refresh & Job Portal Platform",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run backend\" \"npm run frontend\"",
    "backend": "cd backend && npm run dev",
    "frontend": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "start": "cd backend && npm start",
    "test": "concurrently \"cd backend && npm test\" \"cd frontend && npm test\"",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "lint": "concurrently \"cd backend && npm run lint\" \"cd frontend && npm run lint\"",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "clean": "rm -rf node_modules backend/node_modules frontend/node_modules",
    "reset": "npm run clean && npm run install:all",
    "seed": "cd backend && npm run seed",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down"
  },
  "keywords": [
    "ai",
    "resume",
    "job-portal",
    "matching",
    "recruitment",
    "mern-stack",
    "react",
    "nodejs",
    "mongodb",
    "machine-learning"
  ],
  "author": "Resume Refresh Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/resume-refresh-platform.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/resume-refresh-platform/issues"
  },
  "homepage": "https://github.com/your-username/resume-refresh-platform#readme",
  "dependencies": {
    "axios": "^1.12.2"
  }
}
